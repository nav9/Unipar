perspective(x,y,z,x2,y2,z2,tx,ty,tz)
int x,y,z,x2,y2,z2;
float tx,ty,tz;
{
int Z=500,XC=240,YC=250;
float i,j,k;
float r,c,X,Y;
if (z>=Z) {printf("move the observation point farther");getch();return(0);}
i=x;j=-y;k=z;
rot3d(&i,&j,&k,tx,1);
rot3d(&i,&j,&k,ty,2);
rot3d(&i,&j,&k,tz,3);
if (beam.perspective==1)
   {
    r=(Z*j)/(Z-k)+YC;
    c=(Z*i)/(Z-k)+XC;
   }
else {r=j+YC;c=i+XC;}
i=x2;j=-y2;k=z2;
rot3d(&i,&j,&k,tx,1);
rot3d(&i,&j,&k,ty,2);
rot3d(&i,&j,&k,tz,3);
if (beam.perspective==1)
   {
    Y=(Z*j)/(Z-k)+YC;
    X=(Z*i)/(Z-k)+XC;
   }
else {X=i+XC;Y=j+YC;}
line(c,r,X,Y);
}/*--end of p--*/
