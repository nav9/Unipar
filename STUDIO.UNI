button(c,r,C,R,fg,bg,border)
int c,r,C,R;
int fg,bg,border;
{
 int k,i;
 k=(R-r)/2;
 for(i=1;i<3;i++)
 {
  if (i==1) setcolor(bg); else setcolor(border);
  line(c+k,r,C-k,r);
  line(c+k,R,C-k,R);
  arc(c+k,r+k,90,270,k);
  arc(C-k,r+k,270,450,k);
  setfillstyle(SOLID_FILL,bg);
  if (i==1) floodfill(c+k,r+k,bg);
 }
 text(r+6,midpoint(c,C),fg,bg);
}/*-------end of button-----*/
